<UserControl x:Class="Galaxy_Swapper_v2.Workspace.Components.CLobbyControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Galaxy_Swapper_v2.Workspace.Components"
             xmlns:effects="clr-namespace:System.Windows.Media.Effects;assembly=PresentationCore"
             mc:Ignorable="d" 
             x:Name="root"
             Width="190"
             Height="100"
             MouseEnter="root_MouseEnter"
             MouseLeave="root_MouseLeave"
             MouseLeftButtonDown="root_Click"
             Cursor="Hand"
             >
    <Grid>
        <Image x:Name="Logo" Stretch="Fill" Source="https://i.imgur.com/PrJwa2H.jpeg"/>
    </Grid>
</UserControl>